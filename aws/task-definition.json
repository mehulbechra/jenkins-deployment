{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "jenkins-prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "853329776986.dkr.ecr.us-east-1.amazonaws.com/jenkins:1.0.39",
            "portMappings": [{
                "name": "nginx-80-tip",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::853329776986:role/ecsTaskExecutionRole"
}